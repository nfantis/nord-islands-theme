  "tokenColors": [
    {
      "name": "Default text",
      "scope": ["source", "text"],
      "settings": {
        "foreground": "#2E3440"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8790A3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Doc comment tags",
      "scope": [
        "storage.type.class.jsdoc",
        "keyword.other.documentation",
        "punctuation.definition.block.tag.jsdoc",
        "comment.block.documentation storage.type",
        "keyword.other.phpdoc"
      ],
      "settings": {
        "foreground": "#527A7A",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Doc comment tag values",
      "scope": [
        "variable.other.jsdoc",
        "entity.name.type.instance.jsdoc",
        "comment.block.documentation variable",
        "comment.block.documentation entity.name.type"
      ],
      "settings": {
        "foreground": "#4E8E9E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Doc markup",
      "scope": ["comment.block.documentation markup", "comment.block.documentation text.html"],
      "settings": {
        "foreground": "#5E81AC"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#5E81AC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.ternary",
        "keyword.operator.spread",
        "keyword.operator.rest",
        "keyword.operator.type"
      ],
      "settings": {
        "foreground": "#5E81AC",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#4D7A39"
      }
    },
    {
      "name": "String escapes",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#B8860E"
      }
    },
    {
      "name": "Invalid string escapes",
      "scope": ["invalid.illegal.unknown-escape"],
      "settings": {
        "foreground": "#A5404B"
      }
    },
    {
      "name": "Template expressions",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#5E81AC",
        "fontStyle": ""
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#9A6E94"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#527A7A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.other.local"
      ],
      "settings": {
        "foreground": "#2E3440"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#2E3440"
      }
    },
    {
      "name": "Object properties / fields",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#2E3440"
      }
    },
    {
      "name": "Function calls",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#4E8E9E"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function.definition",
        "meta.definition.function entity.name.function",
        "meta.definition.method entity.name.function"
      ],
      "settings": {
        "foreground": "#4E8E9E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes / types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#527A7A"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.type.interface",
        "support.type.interface"
      ],
      "settings": {
        "foreground": "#527A7A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type parameters",
      "scope": ["entity.name.type.type-parameter"],
      "settings": {
        "foreground": "#527A7A",
